<% if(typeof(errors) != 'undefined'){ %>
  <% errors.forEach(function(error) { %>
    <p class="error-msg"><%= error.msg %></p>
  <% }); %>
<% } %>

<% if(success_msg != ''){ %>
  <p class="success-msg"><%= success_msg %></p>
<% } %>

<% if(error_msg != ''){ %>
    <p class="error-msg"><%= error_msg %></p>
<% } %>

<% if(error != ''){ %>
  <p class="error-msg"><%= error %></p>
<% } %>
